{
  "name": "electron-xml-validator",
  "version": "1.0.0",
  "description": "A xml validatio Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "build_win32": "electron-packager . XMLValidator --platform=win32 --arch=x64 --version=1.8.2 --icon=XML.ico --asar=true --ignore=node_modules/electron-* ",
    "build_linux": "electron-packager . XMLValidator --platform=linux --arch=x64 --version=1.8.2 --icon=XML.jpg --asar=true --ignore=node_modules/electron-* "
  },
  "keywords": [
    "Electron",
    "Nodejs",
    "XML"
  ],
  "author": "Weng Yi Chieh",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.8.2",
    "electron-packager": "^11.1.0"
  },
  "dependencies": {
    "file-system": "^2.2.2",
    "xml2js": "^0.4.19"
  }
}
